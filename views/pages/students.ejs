<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <% include ../partial/linkinhead %>
<% include ../partial/navbar %> 
<title>Document</title>
</head>
<body>
<div class="container">
<table class="table">
<thead>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Firstname</th>
        <th>Lastname</th>
    </tr>
</thead>
<tbody>
<% students.forEach(function(student){ %>
    <tr>
        <td scope="row"><%= student.sid%></td>
        <td><%= student.title%></td>
        <td><%= student.firstname%></td>
        <td><%= student.lastname%></td>
    </tr>
    <% }); %>
</tbody>
<div class="container">
</body>
<% include ../partial/linkunderbody %> 
</html>